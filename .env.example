# PostgreSQL Configuration
DB_HOST=your_db_host
DB_NAME=your_db_name
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_PORT=5432
DB_SSL=true

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Other Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database

# Logging Configuration
LOG_LEVEL=info

# Transfer Portal Configuration
TRANSFER_PORTAL_REFRESH_INTERVAL=3600000  # 1 hour in milliseconds

# Notion API Settings
NOTION_API_KEY=your_notion_api_key
NOTION_DATABASE_ID=your_database_id_here

# Cache Settings
CACHE_EXPIRY=3600  # 1 hour in seconds

# Server Settings
MCP_HOST=0.0.0.0
MCP_PORT=8000
MCP_RELOAD=false

# Scraping Settings
SCRAPING_TIMEOUT=30  # seconds
MAX_RETRIES=3

# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key

# Backup Database Configuration
BACKUP_DB_HOST=your_backup_db_host
BACKUP_DB_PORT=5432
BACKUP_DB_USER=your_backup_db_user
BACKUP_DB_PASSWORD=your_backup_db_password
BACKUP_DB_NAME=your_backup_db_name

# Notion Integration Configuration
NOTION_WORKSPACE_ID=your_notion_workspace_id

# Firecrawl Configuration
FIRECRAWL_API_KEY=your_firecrawl_api_key

# GitHub Configuration
GITHUB_TOKEN=your_github_personal_access_token 